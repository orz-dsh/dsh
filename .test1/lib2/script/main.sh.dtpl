#!/bin/sh

lib2_test() {
  echo "lib2 test"
  echo "lib2 get option['_os']: {{ .option._os }}"
  echo "lib2 get option['_arch']: {{ .option._arch }}"
  echo "lib2 get option['_executor']: {{ .option._executor }}"
  echo "lib2 get option['_hostname']: {{ .option._hostname }}"
  echo "lib2 get option['_username']: {{ .option._username }}"
  echo "lib2 get option['test']: {{ .option.test }}"
}
